@using Markdoc.Core.Markdown

<div>
    @((MarkupString)Html)
</div>

@code {

    [Parameter]
    public string FilePath { get; set; } = "";

    private string Html { get; set; } = "";

    protected override Task OnInitializedAsync()
    {
        var reader = new Reader(FilePath);
        Html = reader.ToHtml();
        return Task.CompletedTask;
    }
}
